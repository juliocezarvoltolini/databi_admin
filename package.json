{
  "name": "dashboard-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test-types": "npx tsx scripts/test-types.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed-js": "node prisma/seed.js",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:deploy": "npx prisma migrate deploy",
    "compile-seed": "npx tsc prisma/seed.ts --outDir prisma --target es2020 --module commonjs --esModuleInterop --skipLibCheck"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.77",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.13.0",
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "next": "15.4.6",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.13.0",
    "typescript": "^5"
  }
}
